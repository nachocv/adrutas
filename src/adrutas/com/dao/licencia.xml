<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="licencia">
  <select id="get" resultType="java.util.HashMap">
    SELECT * FROM licencias
  </select>

  <select id="getByAnyo" parameterType="java.lang.Integer" resultType="java.util.HashMap">
    SELECT * FROM licencias where anyo=#{anyo}
  </select>

  <select id="getOpciones" parameterType="java.lang.Integer" resultType="java.util.HashMap">
    SELECT * FROM licencia_opciones where anyo=#{anyo}
  </select>

  <select id="getOpcionesLicencia" parameterType="java.util.HashMap" resultType="java.util.HashMap">
    SELECT * FROM licencia_opciones where anyo=#{anyo} and tipo_licencia=#{tipo_licencia}
  </select>
</mapper>
